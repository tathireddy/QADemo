<suite name="LoginTestCase" verbose="1" >

  <test name="Login Test Cases in Firefox" >
  <parameter name = "browser" value="firefox"/>
    <classes>
       <class name="com.selenium.testing.Login">
        <methods>
         <include name="validateTitle" />
        <exclude name="fbTest" />
      </methods>
       </class>
    </classes>
  </test>
  
  <test name="Login Test Cases in chrome" >
  <parameter name = "browser" value="chrome"/>
    <classes>
       <class name="com.selenium.testing.Login">
        <methods>
        <include name="validateTitle" />
        <exclude name="fbTest" />
      </methods>
       </class>
    </classes>
  </test>
  
  
</suite>